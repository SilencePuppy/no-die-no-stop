<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hc.mybatis.spring.dao.UserMapper">
    <select id="count" resultType="Integer">
       select count(*) form t_user
    </select>

    <select id="listAll" resultType="com.hc.mybatis.spring.entity.UserEntity">
        select * from t_user
    </select>
</mapper>